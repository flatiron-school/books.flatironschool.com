<div class="container">
  <div class="row" id="admin-wrapper">
    <div class="col-md-4 highlight">
      <h4>Admin Page Features</h4>
      <div class="list-group-item grey-background">
        <p>Admin - Add or Remove</p>
        <p>Authorize organizations</p>
        <p>Search - By Books or Students</p>
        <p>Tags - Add</p>
        <p>Upload Files</p>
        </ul>
      </div>
    </div>
    <div class="col-md-4">
      <%= render "admin_list" %>
    </div>
    <div class="col-md-4">
      <%= render "orgs_list" %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-4">
      <%= render "stats" %>
    </div>
    <div class="col-md-4">
      <%= render "create_tags" %>
    </div>
    <div class="col-md-4">
      <h4>Uploads</h4>
      <div class="list-group-item">
        <% unless @incompletes.empty? %>
        <%= link_to "resolve incomplete files", types_path %>
        <% end %> 
        <%= render "types/upload_form" %>
      </div>
    </div>
  </div>

</div>